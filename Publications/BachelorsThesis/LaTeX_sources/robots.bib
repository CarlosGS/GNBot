@incollection{FoxKo05,
year={2005},
abstract={We present an application of hierarchical Bayesian estimation to robot map building. The revisiting problem occurs when a robot has to decide whether it is seeing a previouslybuilt portion of a map, or is exploring new territory. This is a difficult decision problem, requiring the probability of being outside of the current known map. To estimate this probability, we model the structure of a ”typical” environment as a hidden Markov model that generates sequences of views observed by a robot navigating through the environment. A Dirichlet prior over structural models is learned from previously explored environments. Whenever a robot explores a new environment, the posterior over the model is estimated using Dirichlet hyperparameters. Our approach is implemented and tested in the context of multi-robot map merging, a particularly difficult instance of the revisiting problem. Experiments with robot data show that the technique yields strong improvements over alternative methods.},
isbn={978-3-540-23214-8},
booktitle={Robotics Research. The Eleventh International Symposium},
volume={15},
series={Springer Tracts in Advanced Robotics},
editor={Dario, Paolo and Chatila, Raja},
doi={10.1007/11008941_7},
title={A Hierarchical Bayesian Approach to the Revisiting Problem in Mobile Robot Map Building},
url={http://dx.doi.org/10.1007/11008941_7},
publisher={Springer Berlin Heidelberg},
author={Fox, Dieter and Ko, Jonathan and Konolige, Kurt and Stewart, Benjamin},
pages={60-69},
language={English}
}


