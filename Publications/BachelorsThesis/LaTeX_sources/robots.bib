@ARTICLE{YanezToledano12,
  author = {Y\'anez, David Jes\'us and Toledano, Adolfo and Serrano, Eduardo and
    Martin de Rosales, Ana Maria and Rodriguez, Francisco Borja and
    Varona, Pablo},
  title = {Characterization of a clinical olfactory test with an artificial
    nose},
  journal = {Frontiers in Neuroengineering},
  year = {2012},
  volume = {5},
  number = {1},
  abstract = {Clinical olfactory tests are used to address hyposmia/anosmia levels
    in patients with different types of olfactory impairments. Typically,
    a given test is employed clinically and then replaced by a new one
    after a certain period of use which can range from days to several
    months. There is a need to assess control quality of these tests
    and also for a procedure to quantify their degradation over time.
    In this paper we propose a protocol to employ low cost artificial
    noses for the quantitative characterization of olfactory tests used
    in clinical studies. In particular, we discuss a preliminary study
    on the Connecticut Chemosensorial Clinical Research Center Test kit
    which shows that some odorants, as sensed by an artificial nose,
    seem to degrade while others are potentiated as the test ages. We
    also discuss the need to establish a map of correspondence between
    human and machine olfaction when artificial noses are used to characterize
    or compare human smell performance in research and clinical studies.},
  doi = {10.3389/fneng.2012.00001},
  issn = {1662-6443},
  url = {http://www.frontiersin.org/neuroengineering/10.3389/fneng.2012.00001/abstract}
}


