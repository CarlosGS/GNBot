publisher = {OPTICAL SOC AMER},
title = {{Particle-swarm optimization of broadband nanoplasmonic arrays}},
volume = {35},
year = {2010}
}
@article{Reynolds2013,
abstract = {Much progress has been made in understanding how a few informed individuals are able to influence the foraging behaviour of a group and enhance its ability to navigate towards a resource. This research has provided valuable insights into the mechanisms of effective leadership. Here using a simple exactly solvable model we show how effective leadership could operate when no individual in the group has pertinent information about the locations of resources. Simple interactions between an ignorant leader and its followers are found to result in L\'{e}vy walk movement patterns that can optimize random searches. The findings may account for the L\'{e}vy walk movement patterns seen in some marine predators and for the frequent formation of groups of 2 or 3 ungulates when resources become scarce.},
author = {Reynolds, A. M.},
doi = {10.1209/0295-5075/102/18001},
file = {:C$\backslash$:/Users/Pablo Varona/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Reynolds - 2013 - Effective leadership in animal groups when no individual has pertinent information about resource locations How intera.pdf:pdf},
issn = {0295-5075},
journal = {EPL},
language = {en},
month = apr,
number = {1},
pages = {18001},
publisher = {IOP Publishing},
title = {{Effective leadership in animal groups when no individual has pertinent information about resource locations: How interactions between leaders and followers can result in L\'{e}vy walk movement patterns}},
url = {http://iopscience.iop.org/0295-5075/102/1/18001/article/},
volume = {102},
year = {2013}
}

